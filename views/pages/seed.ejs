<%- include('header') -%>
<%- include('navbar') -%>

<style type="text/css">
   .table td {
     text-align: center;   
  } 

   .table th {
     text-align: center;   
  } 
</style>

<div class="container">
  <h1>List Of Seeded Users: </h1>
  <br>

  <table class="table table-hover">
    <tr>
      <th>email</th> 
      <th>password</th> 
    </tr>

    <% for(var i= 0; i < users.length; i++) { %>
      <tr>
        <td><%= users[i].email %></td>
        <td><%= users[i].password %></td>
      </tr>
    <% } %>

    
  </table>
</div>


